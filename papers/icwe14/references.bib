	date-modified = "2011-12-16 12:52:21 +0100",
	location = "Boston, MA",
	numpages = "1",
	pages = "8--8",
	publisher = "USENIX Association",
	series = "{WebApps'10}",
	title = "{SVC: selector-based view composition for web frameworks}",
	url = "http://dl.acm.org/citation.cfm?id=1863166.1863174",
	year = "2010"
}

